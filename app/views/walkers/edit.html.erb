<h2>Edit Details for <%= @walker.name %></h2>

<%= form_for(@walker) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :bio %>
  <%= f.text_area :bio %>
  <%= f.label :years_experience %>
  <%= f.number_field :years_experience, step: 1.0 %>
  <%= f.submit "Submit" %>
  <% end %>
